	#name : #BMIcons,
	#superclass : #Object,
	#classInstVars : [
		'appIcon'
	],
	#category : #'Bruce-Mindmap-icons'
}

{ #category : #accessing }
BMIcons class >> appIcon [ 
	^appIcon ifNil: [ appIcon := Form fromBinaryStream: self appIconStream base64Decoded readStream ]

]

